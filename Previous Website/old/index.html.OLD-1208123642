<!-- Fshije kete script nga browser-at qe nuk suportojne javascripts
  function Validate()
  {
  formObj = document.forms["contactform"];
  var errorMessage = "Please check that u filled all the required fields:\r\n\r\n";
  var okToSubmit = true;
  if (formObj.elements["emri"].value == "") {
    okToSubmit = false;
    errorMessage += "  Please write your name\r\n";
  }
  if (formObj.elements["email"].value == "") {
    okToSubmit = false;
    errorMessage += "  Please write your email address\r\n";
  }
  if (formObj.elements["telefoni"].value == "") {
    okToSubmit = false;
    errorMessage += "  Please write your telephone number\r\n";
  }
  if (formObj.elements["fax"].value == "") {
    okToSubmit = false;
    errorMessage += "  Please write your fax number\r\n";
  }
  if (formObj.elements["mesazhi"].value == "") {
    okToSubmit = false;
    errorMessage += "  Plese write your scaffolding requirements or questions\r\n";
  }
  if (!okToSubmit) {
    errorMessage += "\r\nPlease correct all your errors and resent the formular!";
    alert(errorMessage);
  }
  return okToSubmit;
}
